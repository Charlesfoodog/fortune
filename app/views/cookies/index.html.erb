<h1> welcome to fortune Vault </h1>
 
 <h1>Listing cookies</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @cookies.each do |cookie| %>
      <tr>
        <td><%= cookie.title %></td>
        <td><%= cookie.body %></td>
        <td><%= link_to 'Show', cookie %></td>
        <td><%= link_to 'Edit', edit_cooky_path(cookie) %></td>
        <td><%= link_to 'Destroy', cookie, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cookie', new_cooky_path %>
